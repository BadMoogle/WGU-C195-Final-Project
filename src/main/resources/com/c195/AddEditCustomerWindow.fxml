<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="314.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.c195.windowcontrollers.AddEditCustomerWindowController">
   <children>
      <Label layoutX="48.0" layoutY="60.0" text="%name" />
      <TextField fx:id="textBoxName" layoutX="131.0" layoutY="56.0" prefHeight="25.0" prefWidth="204.0" />
      <Label layoutX="47.0" layoutY="94.0" text="%address" />
      <TextField fx:id="textBoxAddress" layoutX="131.0" layoutY="90.0" prefHeight="25.0" prefWidth="204.0" />
      <Label layoutX="48.0" layoutY="126.0" text="%postalCode" />
      <TextField fx:id="textBoxPostalCode" layoutX="131.0" layoutY="122.0" prefHeight="25.0" prefWidth="204.0" />
      <Label layoutX="48.0" layoutY="160.0" text="%phoneNumber" />
      <TextField fx:id="textBoxPhoneNumber" layoutX="131.0" layoutY="156.0" prefHeight="25.0" prefWidth="204.0" />
      <ComboBox fx:id="comboBoxDivision" layoutX="130.0" layoutY="225.0" prefHeight="25.0" prefWidth="204.0" />
      <Label layoutX="48.0" layoutY="229.0" text="%division" />
      <Button fx:id="buttonSave" layoutX="183.0" layoutY="267.0" mnemonicParsing="false" onAction="#onButtonSaveClicked" prefHeight="25.0" prefWidth="64.0" text="%save" />
      <Button fx:id="buttonCancel" layoutX="261.0" layoutY="267.0" mnemonicParsing="false" onAction="#onButtonCancelClicked" prefHeight="25.0" prefWidth="73.0" text="%cancel" />
      <Label layoutX="48.0" layoutY="194.0" text="%country" />
      <ComboBox fx:id="comboBoxCountry" layoutX="130.0" layoutY="190.0" onAction="#onComboBoxCountryAction" prefHeight="25.0" prefWidth="204.0" />
      <Label layoutX="48.0" layoutY="26.0" text="ID" />
      <TextField fx:id="textBoxId" editable="false" layoutX="131.0" layoutY="22.0" prefHeight="25.0" prefWidth="204.0" />
   </children>
</AnchorPane>
